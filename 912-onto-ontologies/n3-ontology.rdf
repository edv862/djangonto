mssn:HarmonicSpectralDeviation a owl:Class ;
    rdfs:subClassOf mssn:AudioDescriptor .

mssn:HarmonicSpectralSpread a owl:Class ;
    rdfs:subClassOf mssn:AudioDescriptor .

mssn:HarmonicSpectralVariation a owl:Class ;
    rdfs:subClassOf mssn:AudioDescriptor .

mssn:Image a owl:Class ;
    rdfs:subClassOf mssn:MediaConcerned .

mssn:LogAttackTime a owl:Class ;
    rdfs:subClassOf mssn:AudioDescriptor .

mssn:MEETS a owl:Class ;
    rdfs:subClassOf mssn:TemporalOperator .

mssn:METBY a owl:Class ;
    rdfs:subClassOf mssn:TemporalOperator .

mssn:MediaDurationDescriptor a owl:Class ;
    rdfs:subClassOf mssn:TemporalDescriptor .

mssn:MediaTimePointDescriptor a owl:Class ;
