mssn: a owl:Ontology ;
    rdfs:comment "MSSN-Ontology Updated,  August 2017" ;
    owl:imports sosa:,
        ssn: ;
    owl:versionIRI <http://mssn.sigappfr.org/mssn/1.0.1> .

mssn:AtomicEventCondition a owl:Class ;
    rdfs:comment "An atomic event condition is a preprogrammed black-box function that is used for detecting an atomic event" ;
    rdfs:subClassOf sosa:Process .

mssn:Audio a owl:Class ;
    rdfs:subClassOf mssn:MediaConcerned .

mssn:AudioSegment a owl:Class ;
    rdfs:comment "An audio segment is a segment for modeling audio data of MultimediaData" ;
    rdfs:subClassOf mssn:MediaSegment .

mssn:BoundingBoxDescriptor a owl:Class ;
    rdfs:subClassOf mssn:SpatialDescriptor .

mssn:CONTAINS a owl:Class ;
    rdfs:subClassOf mssn:TemporalOperator .

mssn:CameraMotionDescriptor a owl:Class ;
    rdfs:subClassOf mssn:MotionDescriptor .

mssn:ColorLayoutDescriptor a owl:Class ;
    rdfs:subClassOf mssn:VisualDescriptor .

mssn:ColorStructureDescriptor a owl:Class ;
    rdfs:subClassOf mssn:VisualDescriptor .

mssn:ComplexEventOccurrence a owl:Class ;
