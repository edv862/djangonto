    rdfs:subClassOf mssn:TemporalOperator .

mssn:ParametricMotionDescriptor a owl:Class ;
    rdfs:subClassOf mssn:MotionDescriptor .

mssn:PointDescriptor a owl:Class ;
    rdfs:subClassOf mssn:SpatialDescriptor .

mssn:PowerDescriptor a owl:Class ;
    rdfs:subClassOf mssn:AudioDescriptor .

mssn:SEQ a owl:Class ;
    rdfs:comment "An event operator that is used for expressing that two event occurrences are followed each other." ;
    rdfs:subClassOf mssn:TemporalOperator .

mssn:STARTEDBY a owl:Class ;
    rdfs:subClassOf mssn:TemporalOperator .

mssn:STARTS a owl:Class ;
    rdfs:subClassOf mssn:TemporalOperator .

mssn:ScalableColorDescriptor a owl:Class ;
    rdfs:subClassOf mssn:VisualDescriptor .

mssn:SpatialOperator a owl:Class ;
