mssn:EdgeHistogramDescriptor a owl:Class ;
    rdfs:subClassOf mssn:VisualDescriptor .

mssn:EventStatement a owl:Class ;
    rdfs:comment "An event state is a user-defined statement that is used for producing an event occurrence.   An event statement is modeled by using the CEMiD Language" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mssn:produceOccurrence ;
            owl:someValuesFrom mssn:EventOccurrence ],
        sosa:Process .

mssn:FINISHEDBY a owl:Class ;
    rdfs:subClassOf mssn:TemporalOperator .

mssn:FINISHES a owl:Class ;
    rdfs:subClassOf mssn:TemporalOperator .

mssn:FaceRecognitionDescriptor a owl:Class ;
    rdfs:subClassOf mssn:VisualDescriptor .

mssn:FundamentalFrequency a owl:Class ;
    rdfs:subClassOf mssn:AudioDescriptor .

mssn:HarmonicDescriptor a owl:Class ;
    rdfs:subClassOf mssn:AudioDescriptor .

mssn:HarmonicSpectralCentroid a owl:Class ;
