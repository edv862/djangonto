mssn:directionalRelation a owl:ObjectProperty ;
    vann:preferredNamespaceUri "An abstract relation for directional location relations" ;
    rdfs:subPropertyOf mssn:locationRelation .

mssn:MediaDescriptor a owl:Class ;
    rdfs:subClassOf sosa:ObservationValue .

mssn:VisualDescriptor a owl:Class ;
    rdfs:subClassOf mssn:MediaDescriptor .

mssn:topologicalRelation a owl:ObjectProperty ;
    vann:preferredNamespaceUri "An abstract relation for topological location relations" ;
    rdfs:subPropertyOf mssn:locationRelation .

mssn:TemporalOperator a owl:Class ;
    rdfs:subClassOf mssn:EventOperator .

mssn:AudioDescriptor a owl:Class ;
    rdfs:subClassOf mssn:MediaDescriptor .

