<?xml version="1.0"?>
<rdf:RDF xmlns="http://mssn.sigappfr.org/mssn/"
     xml:base="http://mssn.sigappfr.org/mssn/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:mssn="http://mssn.sigappfr.org/mssn/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/">
    <owl:Ontology rdf:about="http://mssn.sigappfr.org/mssn/">
        <owl:versionIRI rdf:resource="http://mssn.sigappfr.org/mssn/1.0.1"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/sosa/"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/ssn/"/>
        <rdfs:comment>MSSN-Ontology Updated,  August 2017</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
    


    <!-- http://schema.org/domainIncludes -->

    <owl:AnnotationProperty rdf:about="http://schema.org/domainIncludes"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#note -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#note"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://mssn.sigappfr.org/mssn/above -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/above">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/directionalRelation"/>
        <rdfs:comment>A directional relation for indicating a location is located above another location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/below -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/below">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/directionalRelation"/>
        <rdfs:comment>A directional relation for indicating a location is located below another location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/closeTo -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/closeTo">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/distanceRelation"/>
        <rdfs:comment>A distance relation for indicating a location is located closed to another location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/contains -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/contains">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:comment>A topological relation for indicating that a location contains an another location (e.g. Room1 contains Table1)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/covers -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/covers">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:comment>an extended definition of &quot;contains&quot; for indicating all spatial area of a given location lies completely within another location. For example, A covers B means all the area of B lies totally within an area of A.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/crosses -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/crosses">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:comment>A topological relation for indicating that and area of a given location crosses with an area of another location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/directionalRelation -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/directionalRelation">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/locationRelation"/>
        <vann:preferredNamespaceUri>An abstract relation for directional location relations</vann:preferredNamespaceUri>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/disjoint -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/disjoint">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:comment>A topological relation for indicating that two locations are disjoint with each other.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/distanceRelation -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/distanceRelation">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/locationRelation"/>
        <vann:preferredNamespaceUri>An abstract relation for distance relations</vann:preferredNamespaceUri>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/equals -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/equals">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:comment>A topological relation for indicating that two locations are exactly the same.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/farFrom -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/farFrom">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/distanceRelation"/>
        <rdfs:comment>A directional relation for indicating that a location is far from another location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasCoverageArea -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/hasCoverageArea">
        <rdfs:comment>A relation between a sensor and a coverage area. It uses for indicating coverage areas for a sensor</rdfs:comment>
        <rdfs:label>has coverage area</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasLocation -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/hasLocation">
        <rdfs:comment>A relation between LocationMap and Location for indicating that a given location map contains which locations</rdfs:comment>
        <rdfs:label>has location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasMediaConcerned -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/hasMediaConcerned">
        <rdfs:comment>A relation between a mediasensor and mediaconcerned concepts</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasMediaDescriptor -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/hasMediaDescriptor">
        <rdfs:comment>A relation between a media segment and a media descriptor</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasMediaSegment -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/hasMediaSegment">
        <rdfs:comment>A relation between MultimediaData and MediaSegment concept</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasObeservationValue -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/hasObeservationValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:comment>A relation between an atomic event occurrence and an observation value for indicating that a given atomic event occurrence is related to such an observation value.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/leftOf -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/leftOf">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/directionalRelation"/>
        <rdfs:comment>A directional relation for indicating a location is located on the left of location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/locationRelation -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/locationRelation">
        <rdfs:comment>A location relation describes a relation between a pair of locations e.g. &quot;Table1&quot; opposite &quot;Table2&quot;.   

A location relation can be either topological relation, directional relation or distance relation.

This relation intends to be use as an abstract relation for other location relations.   Hence, it may not be used directly.    Only sub-relations of this relation should be used</rdfs:comment>
        <rdfs:label>Location Relation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/operand -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/operand">
        <rdfs:comment>A relation from an event operator to an event occurrence</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/operator -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/operator">
        <rdfs:comment>A relation from an event occurrence to an event operator</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/opposite -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/opposite">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/directionalRelation"/>
        <rdfs:comment>A directional relation for indicating a location is opposite another location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/overlaps -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/overlaps">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:comment>A location relation for indicating that two locations are overlapped with each other.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/produceOccurrence -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/produceOccurrence">
        <rdfs:comment>A relation between an atomic event statement and an event occurrence</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/rightOf -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/rightOf">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/directionalRelation"/>
        <rdfs:comment>A directional relation for indicating a location is located on the right of another location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/topologicalRelation -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/topologicalRelation">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/locationRelation"/>
        <vann:preferredNamespaceUri>An abstract relation for topological location relations</vann:preferredNamespaceUri>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/touches -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/touches">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:comment>A topological relation for indicating that boundary of two locations touch each other</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://mssn.sigappfr.org/mssn/hasAdditionalParams -->

    <owl:DatatypeProperty rdf:about="http://mssn.sigappfr.org/mssn/hasAdditionalParams">
        <rdfs:comment>A data property for specifying additional parameters for AtomicEventCondition</rdfs:comment>
        <rdfs:label>has additional params</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasIRI -->

    <owl:DatatypeProperty rdf:about="http://mssn.sigappfr.org/mssn/hasIRI">
        <rdfs:comment>A data property for attaching an IRI literal to a concept.  It is mostly used for attaching an IRI to AtomicEventCondition</rdfs:comment>
        <rdfs:label>has IRI</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasOperatorName -->

    <owl:DatatypeProperty rdf:about="http://mssn.sigappfr.org/mssn/hasOperatorName">
        <rdfs:comment>A data property for indicating an operator name for a spatial operator</rdfs:comment>
        <rdfs:label>has operator name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/statement -->

    <owl:DatatypeProperty rdf:about="http://mssn.sigappfr.org/mssn/statement">
        <rdfs:comment>A data property for specifying a statement for an EventStatement</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://mssn.sigappfr.org/mssn/AnnotationDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/AnnotationDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/MediaDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/AtomicEventCondition -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/AtomicEventCondition">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Process"/>
        <rdfs:comment>An atomic event condition is a preprogrammed black-box function that is used for detecting an atomic event</rdfs:comment>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/AtomicEventOccurrence -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/AtomicEventOccurrence">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/EventOccurrence"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/Audio -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/Audio">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/MediaConcerned"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/AudioDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/AudioDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/MediaDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/AudioSegment -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/AudioSegment">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/MediaSegment"/>
        <rdfs:comment>An audio segment is a segment for modeling audio data of MultimediaData</rdfs:comment>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/BoundingBoxDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/BoundingBoxDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/SpatialDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/CONTAINS -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/CONTAINS">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/TemporalOperator"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/CameraMotionDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/CameraMotionDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/MotionDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/ColorLayoutDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/ColorLayoutDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/VisualDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/ColorStructureDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/ColorStructureDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/VisualDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/ComplexEventOccurrence -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/ComplexEventOccurrence">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/EventOccurrence"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/ContourShapeDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/ContourShapeDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/VisualDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/CoverageArea -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/CoverageArea">
        <owl:equivalentClass rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <schema:domainIncludes>Coverage Area</schema:domainIncludes>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/DURING -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/DURING">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/TemporalOperator"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/DominatColorDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/DominatColorDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/VisualDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/EQUALS -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/EQUALS">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/TemporalOperator"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/EdgeHistogramDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/EdgeHistogramDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/VisualDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/EventOccurrence -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/EventOccurrence">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://mssn.sigappfr.org/mssn/operator"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://mssn.sigappfr.org/mssn/EventOperator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/EventOperator -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/EventOperator">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://mssn.sigappfr.org/mssn/operand"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://mssn.sigappfr.org/mssn/EventOccurrence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An event operator is a binary operator that is described the relation between a pair of event occurrences.   An operator can be either spatial or temporal</rdfs:comment>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/EventStatement -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/EventStatement">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://mssn.sigappfr.org/mssn/produceOccurrence"/>
                <owl:someValuesFrom rdf:resource="http://mssn.sigappfr.org/mssn/EventOccurrence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An event state is a user-defined statement that is used for producing an event occurrence.   An event statement is modeled by using the CEMiD Language</rdfs:comment>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/FINISHEDBY -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/FINISHEDBY">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/TemporalOperator"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/FINISHES -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/FINISHES">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/TemporalOperator"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/FaceRecognitionDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/FaceRecognitionDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/VisualDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/FundamentalFrequency -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/FundamentalFrequency">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/AudioDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/HarmonicDescriptor -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/HarmonicDescriptor">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/AudioDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/HarmonicSpectralCentroid -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/HarmonicSpectralCentroid">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/AudioDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/HarmonicSpectralDeviation -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/HarmonicSpectralDeviation">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/AudioDescriptor"/>
    </owl:Class>
    


    <!-- http://mssn.sigappfr.org/mssn/HarmonicSpectralSpread -->

    <owl:Class rdf:about="http://mssn.sigappfr.org/mssn/HarmonicSpectralSpread">
        <rdfs:subClassOf rdf:resource="http://mssn.sigappfr.org/mssn/AudioDescriptor"/>
